{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "components/*": [
        "src/components/*"
      ],
      "shared/*": [
        "src/shared/*"
      ],
      "modules/*": [
        "src/modules/*"
      ],
      "hooks/*": [
        "src/hooks/*"
      ]
    }
  }
}